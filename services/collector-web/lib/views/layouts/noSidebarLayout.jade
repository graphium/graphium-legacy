doctype html
html(lang='en')
	block layoutVars
	head
		meta(charset='utf-8')
		link(rel='icon', type='image/png', href='/favicon_new.ico')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		title #{title} - Graphium Health
		meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0', name='viewport')
		meta(name='viewport', content='width=device-width')
		include globalCss.jade
		include globalHeadScripts.jade
	body
		.main-panel(style={width:'100%'})
			include ../components/noSidebarNavbar
			.content(style={'padding-top':'0px'})
				.container-fluid
					block content
			footer.footer
				.container-fluid
					nav.pull-left
					p.copyright.pull-right
						- var year = new Date().getFullYear();
						| Â© #{year} 
						a(href='http://www.graphiumhealth.com') Graphium Health
							| , Anesthesia Practice Solutions
		script.
			var htmlEnv = JSON.parse(unescape("#{escape(JSON.stringify(htmlEnv))}"));
			var orgUser = JSON.parse(unescape("#{escape(JSON.stringify(orgUser))}"));
			var indexUser = JSON.parse(unescape("#{escape(JSON.stringify(indexUser))}"));
	include ./hubspot
	block modals
	include globalScripts.jade
	script 
		include authenticatedDefaultLayout.js
	block otherscripts